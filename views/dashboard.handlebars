<section class="container">
  <h3 class="user-dashboard">{{user.username}}</h3>
  <h4>User Posts:</h4>
  <!-- If the user is logged in, render the posts -->
  {{#if loggedIn}}
  <section class="flex-row justify-center">
    {{#each post.user as |post|}}
    {{> post-details}}
    {{/each}}
  </section>
</section>
<!-- Otherwise, provide a link to login -->
{{else}}
<a href="/login">You must login first to view this page</a>
{{/if}}
